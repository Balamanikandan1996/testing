#!/bin/bash
cmd=$(sudo grep "max_log_file.*8" /etc/audit/auditd.conf 2> /dev/null)
if [ $(sudo grep "max_log_file.*8" /etc/audit/auditd.conf 2> /dev/null | wc -l) -eq 0 ]; then
   echo "{ \"auditlogsize_4121_actual\" : \"\" }"
else
   echo "{ \"auditlogsize_4121_actual\" : \"${cmd}\" }"
   exit 1
fi

