#!/bin/bash
cmd=$(grep -rnw '/etc/sysconfig/iptables' -e '# Generated by iptables-save')

if [ "$( grep -rnw '/etc/sysconfig/iptables' -e '# Generated by iptables-save' | wc -l)" -eq 0 ];then
   echo "{ \"iptablesrules_35325_actual\" : \"\"}"
else
   echo "{ \"iptablesrules_35325_actual\" : \"$cmd\"}"
   exit 0
fi

