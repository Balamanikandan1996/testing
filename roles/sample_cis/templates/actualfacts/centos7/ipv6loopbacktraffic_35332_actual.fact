#!/bin/bash

a=$(sudo ip6tables -L INPUT -v -n | egrep ".*ACCEPT.*" | paste -sd "," -)
b=$(sudo ip6tables -L OUTPUT -v -n | egrep  ".*ACCEPT.*" | paste -sd "," -)
cmd="${a}","${b}"
if [ $(sudo ip6tables -L INPUT -v -n | egrep ".*ACCEPT.*" | wc -l) -eq 0 ] || [ $(sudo ip6tables -L OUTPUT -v -n | egrep  ".*ACCEPT.*" | wc -l) -eq 0 ]; then
   echo "{ \"ipv6loopbacktraffic_35332_actual\" : \"\"}"
else
   echo "{ \"ipv6loopbacktraffic_35332_actual\" : \"$cmd\"}"
   exit 0
fi

