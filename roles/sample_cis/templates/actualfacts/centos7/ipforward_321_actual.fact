#!/bin/bash
cmd=$(cat /etc/sysctl.conf | grep -m1 "^net.ipv4.ip_forward=0")
if [ "$(sysctl net.ipv4.ip_forward)" != "net.ipv4.ip_forward = 0" ] || [ $(grep "^net.ipv4.ip_forward=0" /etc/sysctl.conf | wc -l) -eq 0 ]; then
   echo "{ \"ipforward_321_actual\" : \"\" }"
else
   echo "{ \"ipforward_321_actual\" : \"${cmd}\" }"
   exit 1
fi

