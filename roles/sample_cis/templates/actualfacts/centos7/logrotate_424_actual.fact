#!/bin/bash
cmd=$(cat /etc/logrotate.conf | grep "maxage 90")
if [ $(sudo cat /etc/logrotate.conf | tr '\n' '\r' | sed -e 's/{.}//g' | tr '\r' '\n' | grep -v "^\s#" | grep "maxage" | wc -l) -eq 0 ]; then
   echo "{ \"logrotate_424_actual\" : \"\" }"
else
   echo "{ \"logrotate_424_actual\" : \"${cmd}\" }"
   exit 1
fi

