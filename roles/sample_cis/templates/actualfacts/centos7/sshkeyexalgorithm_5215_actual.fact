#!/bin/bash

cmd=$(grep "KexAlgorithms curve25519-sha256*"  /etc/ssh/sshd_config )

if [ $(grep "KexAlgorithms curve25519-sha256*"  /etc/ssh/sshd_config | wc -l) -eq 0 ]; then
   echo "{ \"sshkeyexalgorithm_5215_actual\" : \"\"}"
else
   echo "{ \"sshkeyexalgorithm_5215_actual\" : \"$cmd\"}"
   exit 0
fi

