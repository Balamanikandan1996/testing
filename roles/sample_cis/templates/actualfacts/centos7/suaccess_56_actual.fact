#!/bin/bash
cmd=$(cat /etc/pam.d/su | grep -o "group=sugroup")
if [ $(sudo grep -E '^\s*auth\s+required\s+pam_wheel\.so\s+(\S+\s+)*use_uid\s+(\S+\s+)*group=\S+\s*(\S+\s*)*(\s+#.*)?$' /etc/pam.d/su | wc -l) -eq 0 ]; then
   echo "{ \"suaccess_56_actual\" : \"\" }"
else
   echo "{ \"suaccess_56_actual\" : \"${cmd}\"}"
   exit 0
fi

