#!/bin/bash
NOW=$(date +%s)
for i in $(egrep "^[^:]+:[\$]|^[^:]+:\!\!" /etc/shadow | awk -F: '$6==7 {print $1}'); do
    if [ "$i" != 'root' ]; then
       UPA=$(chage --list $i | grep "Last password change" | cut -d: -f2)
       EPOCH=$(date -d "$UPA" +%s)
    fi
done
if [[ $EPOCH -gt $NOW ]]; then
   echo "{ \"lastpasswdchange_5415_actual\" : \"\" }"
else
   echo "{ \"lastpasswdchange_5415_actual\" : \"$NOW\" }"
fi

